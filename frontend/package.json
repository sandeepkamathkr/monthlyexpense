{
  "name": "monthly-expense",
  "version": "1.0.0",
  "description": "Monthly Expense Tracker",
  "main": "app.js",
  "scripts": {
    "start": "cd ../backend && mvn spring-boot:run",
    "build": "cd ../backend && mvn clean package",
    "test": "cd ../backend && mvn test",
    "eject": "react-scripts eject",
    "serve": "npm run start & while ! nc -z localhost 8081; do sleep 1; done && serve -s .",
    "start-react": "serve -s public/",
    "build-react": "react-scripts build"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "bootstrap": "^5.2.3",
    "chart.js": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "react-scripts": "5.0.1",
    "serve": "^14.2.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8081"
}
